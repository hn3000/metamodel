  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^10.12.18",
    "tslint": "^5.12.1",
    "tsunit.external": "^2.1.1",
    "typescript": "^3.2.2",
    "nyc": "^13.1.0"
  },
  "dependencies": {
    "@hn3000/json-ref": "^1.0.0"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^2.2.1"
